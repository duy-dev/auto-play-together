<Window x:Class="Auto_Demo.draw.rounded"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Auto_Demo.draw"
        mc:Ignorable="d" x:Name="win_Draw"
        Title="draw" Height="800" Width="800" Background="{x:Null}" AllowsTransparency="True" WindowStyle="None">
    <Grid>
        <Border HorizontalAlignment="Left" VerticalAlignment="Top" x:Name="drawRound" Margin="50 50 0 0" BorderBrush="#FFFFFF" Height="100" Width="100" BorderThickness="1" CornerRadius="1000"></Border>
        <Border HorizontalAlignment="Left" VerticalAlignment="Top" x:Name="drawRound2" BorderBrush="#FFFFFF" Height="200" Width="200" BorderThickness="1" CornerRadius="1000"></Border>
    </Grid>
</Window>
